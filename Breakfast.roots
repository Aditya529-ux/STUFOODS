            border-radius: 10px;
        }
        .food-item h3 {
            margin: 15px 0;
            font-size: 20px;
        }
        .food-item p {
            color: #555;
            font-size: 14px;
        }
        .order-btn {
            background-color: #ff6600;
            color: white;
            border: none;
            padding: 10px 20px;
            border-radius: 5px;
            cursor: pointer;
            margin-top: 10px;
        }
        .order-btn:hover {
            background-color: #e65c00;
        }

        footer {
            
            background-color: #333;
            color: rgb(250, 246, 246);
            text-align: center;
            padding: 15px 0;
            margin-top: 30px;
        }
        footer p {
